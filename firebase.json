{
  "hosting": {
    "public": "build_output",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/proxy",
        "function": "proxy"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
